}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IMG_URL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PRODUCTO.PRD_DESCRIPCION)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IMG_URL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRODUCTO.PRD_DESCRIPCION)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IMG_ID }) |
